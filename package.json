{
  "name": "hlws-bot",
  "version": "0.0.0",
  "type": "module",
  "main": "index.js",
  "description": "HL WebSocket BOT workspace",
  "license": "MIT",
  "bin": {
    "hlb": "./bin/hlb"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "build": "tsc",
    "build:runtime": "bash scripts/ops/build_runtime_dist.sh",
    "build:watch": "tsc --watch",
    "test:ws": "node tests/ws-index-test-runner.js",
    "ws:run": "node tests/ws-index-run.js",
    "stale:check": "node tests/stale-test.js",
    "logic:log": "node tests/logic-log-run.js",
    "ops:sync": "bash scripts/ops/sync_dual_source.sh",
    "ops:drift": "bash scripts/ops/report_dual_source_drift.sh",
    "ops:tune": "node scripts/ops/auto_tune_from_logs.js",
    "ops:tune:apply": "bash scripts/ops/daily_auto_tune.sh",
    "ops:sim-from-events": "node scripts/ops/simulate_from_events.js",
    "ops:raw-backtest": "node scripts/ops/raw_log_backtest.js",
    "ops:raw-backtest:grid": "node scripts/ops/raw_backtest_grid.js",
    "ops:replay": "node scripts/ops/counterfactual_replay.js",
    "ops:replay:apply": "node scripts/ops/counterfactual_replay.js --apply",
    "v2:ws:viz": "node scripts/ws_pressure_visualize.js",
    "v2:eval:truth": "node scripts/validation/ws_event_truth_eval.js",
    "v2:eval:split": "node scripts/validation/split_validation_orchestrator.js",
    "v2:eval:judge": "node scripts/validation/judge_validation_results.js",
    "v2:shadow:summary": "node scripts/validation/shadow_live_summary.js",
    "v2:window:files": "node scripts/ops/select_window_files.js",
    "v2:logs:retention": "bash scripts/ops/log_retention.sh",
    "v2:research:cleanup": "bash scripts/ops/research_cleanup.sh",
    "v2:run:cycle": "bash scripts/ops/run_validation_cycle.sh",
    "v2:run:until": "bash scripts/ops/run_until_target.sh",
    "v2:notify": "bash scripts/ops/notify_discord.sh",
    "research:lead:run": "node scripts/research/ws_lead_multi_horizon_run.js --logs-dir logs --out-dir logs/ops",
    "research:lead:stability:short": "node scripts/research/ws_lead_stability_score.js --in-dir logs/ops --prefix ws_lead_indicator_scan_short --min-days 2 --top 20 --out logs/ops/ws_lead_stability_score_short.json",
    "research:lead:stability:mid": "node scripts/research/ws_lead_stability_score.js --in-dir logs/ops --prefix ws_lead_indicator_scan_mid --min-days 2 --top 20 --out logs/ops/ws_lead_stability_score_mid.json",
    "research:lead:relation": "node scripts/research/ws_lead_horizon_relation.js --in-dir logs/ops --short-prefix ws_lead_indicator_scan_short --mid-prefix ws_lead_indicator_scan_mid --weak-threshold 0.1 --out logs/ops/ws_lead_horizon_relation_latest.json",
    "research:lead:daily": "npm run research:lead:run && npm run research:lead:stability:short && npm run research:lead:stability:mid && npm run research:lead:relation",
    "research:lead:digest:daily": "node scripts/research/ws_lead_digest.js --mode daily --in-dir logs/ops --out logs/ops/ws_lead_digest_daily.txt --json-out logs/ops/ws_lead_digest_daily.json",
    "research:lead:digest:weekly": "node scripts/research/ws_lead_digest.js --mode weekly --in-dir logs/ops --days 7 --out logs/ops/ws_lead_digest_weekly.txt --json-out logs/ops/ws_lead_digest_weekly.json",
    "research:lead:discord:daily": "bash scripts/research/send_ws_lead_digest_discord.sh daily logs/ops/ws_lead_digest_daily.txt",
    "research:lead:discord:weekly": "bash scripts/research/send_ws_lead_digest_discord.sh weekly logs/ops/ws_lead_digest_weekly.txt",
    "research:lead:report:daily": "bash scripts/research/run_ws_lead_reporting.sh daily",
    "research:lead:report:weekly": "bash scripts/research/run_ws_lead_reporting.sh weekly",
    "research:edge:eval": "node scripts/research/ws_state_edge_eval.js",
    "research:edge:sweep": "node scripts/research/ws_edge_sweep.js",
    "research:edge:scan": "node scripts/research/ws_branch_scan.js",
    "research:edge:digest:daily": "node scripts/research/ws_edge_digest.js --mode daily --run-dir logs/ops/ws_edge_pipeline/latest --out logs/ops/ws_edge_digest_daily.txt --json-out logs/ops/ws_edge_digest_daily.json",
    "research:edge:digest:weekly": "node scripts/research/ws_edge_digest.js --mode weekly --run-dir logs/ops/ws_edge_pipeline/latest --out logs/ops/ws_edge_digest_weekly.txt --json-out logs/ops/ws_edge_digest_weekly.json",
    "research:edge:discord:daily": "bash scripts/research/send_ws_edge_digest_discord.sh daily logs/ops/ws_edge_digest_daily.txt",
    "research:edge:discord:weekly": "bash scripts/research/send_ws_edge_digest_discord.sh weekly logs/ops/ws_edge_digest_weekly.txt",
    "research:edge:report:daily": "bash scripts/research/run_ws_edge_reporting.sh daily",
    "research:edge:report:weekly": "bash scripts/research/run_ws_edge_reporting.sh weekly",
    "research:pattern:train": "node scripts/research/ws_pattern_discovery.js --mode train",
    "research:pattern:apply": "node scripts/research/ws_pattern_discovery.js --mode apply",
    "research:pattern:refresh": "node scripts/research/ws_pattern_discovery.js --mode refresh",
    "research:pattern:digest": "node scripts/research/ws_pattern_digest.js --run-dir logs/ops/ws_pattern_pipeline/latest --out logs/ops/ws_pattern_digest_scheduled.txt --json-out logs/ops/ws_pattern_digest_scheduled.json",
    "research:pattern:discord": "bash scripts/research/send_ws_pattern_digest_discord.sh scheduled logs/ops/ws_pattern_digest_scheduled.txt",
    "research:pattern:report": "bash scripts/research/run_ws_pattern_reporting.sh scheduled",
    "research:pattern:report:daily": "bash scripts/research/run_ws_pattern_reporting.sh daily",
    "research:pattern:report:weekly": "bash scripts/research/run_ws_pattern_reporting.sh weekly",
    "research:waveform:extract": "node scripts/research/ws_waveform_pattern_extract.js",
    "research:waveform:digest": "node scripts/research/ws_waveform_digest.js --run-dir logs/ops/ws_waveform_pipeline/latest --out logs/ops/ws_waveform_digest_scheduled.txt --json-out logs/ops/ws_waveform_digest_scheduled.json",
    "research:waveform:discord": "bash scripts/research/send_ws_waveform_digest_discord.sh scheduled logs/ops/ws_waveform_digest_scheduled.txt",
    "research:waveform:report": "bash scripts/research/run_ws_waveform_reporting.sh scheduled",
    "research:waveform:check": "node scripts/research/ws_waveform_healthcheck.js --latest-dir logs/ops/ws_waveform_pipeline/latest --max-age-hours 16 --out logs/ops/ws_waveform_pipeline/health_latest.json --history logs/ops/ws_waveform_pipeline/health_history.jsonl",
    "research:liq:monitor": "node scripts/research/ws_liq_monitor.js --logs-dir logs --out-dir logs/ops/ws_liq_monitor/latest --max-days 3 --window-sec 10 --burst-usd 100000 --horizons-sec 30,60,180 --cooldown-sec 20",
    "research:liq:wave:join": "node scripts/research/ws_liq_wave_join.js --liq-events-csv logs/ops/ws_liq_monitor/latest/liq_events.csv --wave-events-csv logs/ops/ws_waveform_pipeline/latest/source/events_all.csv --out-dir logs/ops/ws_liq_monitor/latest --match-window-sec 30 --move-bps 5",
    "research:liq:linked": "npm run -s research:liq:monitor && npm run -s research:liq:wave:join",
    "depcruise:b2:local": "dependency-cruiser --config .dependency-cruiser.b2-hub.draft.mjs logic",
    "dep:check:b2hub": "dependency-cruiser --config .dependency-cruiser.b2-hub.draft.mjs logic",
    "test": "NODE_OPTIONS=--experimental-vm-modules LOG_TRADES_PATH=test-logs/jest/trades.jsonl jest",
    "test:normalize": "jest tests/normalize-",
    "research:edge:compare": "node scripts/research/ws_edge_compare.js"
  },
  "dependencies": {
    "axios": "^1.7.9",
    "chalk": "^4.1.2",
    "dotenv": "^16.6.1",
    "ethers": "^6.16.0",
    "eventemitter3": "^5.0.1",
    "minimist": "^1.2.8",
    "node-fetch": "^3.3.2",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "babel-jest": "^30.2.0",
    "dependency-cruiser": "^17.3.8",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "typescript": "^5.9.3"
  }
}
